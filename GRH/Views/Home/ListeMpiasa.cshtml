@{
     Layout = "~/Views/Shared/BackOfficeAccueil.cshtml";
}
<div class="card shadow mb-4">
 <div class="card-header py-3">
   <h6 class="m-0 font-weight-bold text-primary">Liste des employ√©s </h6>
 </div>
 <div class="card-body">
   <div class="table-responsive">
     <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
       <thead>
       <tr>
         <th>Matricule</th>
         <th>Nom</th>
         <th>Prenoms</th>
         <th>Date de naissance</th>
         <th>Adresse</th>
         <th>Date d'embauche</th>
         <th>Poste</th>
         <th>Action</th>
       </tr>
       </thead>
       <tfoot>
       <tr>
         <th>Matricule</th>
         <th>Nom</th>
         <th>Prenoms</th>
         <th>Date de naissance</th>
         <th>Adresse</th>
         <th>Date d'embauche</th>
         <th>Poste</th>
         <th>Action</th>
       </tr>
       </tfoot>
       <tbody>
       @foreach (Mpiasa mp in @ViewBag.allMpiasa)
       {
         <tr>
           <td>MP00 @mp.matricule</td>
           <td>@mp.clients.nom</td>
           <td>@mp.clients.prenoms</td>
           <td>@mp.clients.dtn</td>
           <td>@mp.clients.adresse</td>
           <td>@mp.entre</td>
           <td>@mp.postes.nomPoste</td>
           <td class="action-table">
             <a href="#" class="btn btn-success btn-circle">
               <i class="fas fa-pencil-alt"></i>
             </a>
             <a href="#" class="btn btn-danger btn-circle">
               <i class="fas fa-trash"></i>
             </a>
             <a href="" class="btn btn-info btn-circle">
               <i class="fas fa-info-circle"></i>
             </a>
           </td>
         </tr>
       }
       </tbody>
     </table>
   </div>
 </div>
 </div>